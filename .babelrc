{
  "presets": [
    [
      "next/babel",
      {
        "preset-react": {
          "runtime": "classic"
        }
      }
    ]
  ],
  "plugins": [
    "babel-plugin-transform-typescript-metadata",
    [
      "@babel/plugin-proposal-decorators",
      {
        "legacy": true
      }
    ],
    "babel-plugin-parameter-decorator",
    "inline-react-svg"
  ]
}